    <body>
        <div id="full" class="full">
        <section id="logo-section"class="hero p-2">
            <div class="container">
                <figure class="image center">
                    <img src="images/Logo_White.svg" alt="Camunda logo">
                </figure>
            </div>
        </section>
        <section id="banner-section" class="hero">
            <div class="columns">
                <div class="column is-one-fourth"></div>
                <div class="column is-half">
                    <h1 class="title has-text-white" >Help us translating Camunda applications!</h1>
                </div>
                <div class="column"></div>
                    <div id='select-theme-div' class="select is-small p-2">
                        <select id="select-theme">
                            <option value="" selected="selected">Change theme</option>
                            
                            <option value="stylish">Stylish</option>
                            <option value="purple">Purple</option>
                            <option value="blackWhite">Black & White</option>
                            <option value="striking">Striking</option>
                            <option value="savana">Savana</option>
                            <option value="forest">Forest</option>
                        </select>
                    </div>
                </div>                               
            </div>
